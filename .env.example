# Database
POSTGRES_USER=agent_eval
POSTGRES_PASSWORD=agent_eval_password
POSTGRES_DB=agent_eval
DATABASE_URL=postgresql://agent_eval:agent_eval_password@localhost:5433/agent_eval

# Encryption key for access tokens (32 bytes hex = 64 characters)
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
ENCRYPTION_KEY=your-32-byte-hex-key-here-generate-a-new-one

# JWT Authentication
# Generate secrets with: node -e "console.log(require('crypto').randomBytes(64).toString('hex'))"
JWT_SECRET=your-jwt-secret-here-generate-a-new-one
JWT_REFRESH_SECRET=your-jwt-refresh-secret-here-generate-a-new-one

# Logging level: error, warn, info (default), debug, verbose
# TypeORM query logs only show at debug or verbose levels
LOG_LEVEL=info